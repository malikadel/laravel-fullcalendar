{
    "name": "malikadel/laravel-fullcalendar",
    "description": "Laravel 7/8 helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Mills",
            "email": "alex@asdfx.us"
        },
        {
            "name": "Shawn Tunney",
            "email": "shawn.tunney@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-0": {
            "Asdfx\\LaravelFullcalendar": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asdfx\\LaravelFullcalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "Asdfx\\LaravelFullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
